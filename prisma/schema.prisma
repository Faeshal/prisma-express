// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Client {
  id    Int     @id @default(autoincrement())
  name  String
  email Boolean @default(true)
}

model Item {
  id   Int    @id @default(autoincrement())
  name String
}

model Trip {
  id   Int      @id @default(autoincrement())
  date DateTime
}
